कर्म सूत्र११०(कोशिका, लंबाई){
  
  नित्य नईकोशिका = []। 

  चक्र (मान अ=० । से लंबाई){
    नित्य बाएँ = असत्य। 
    यदि(अ==०)
        {बाएँ = असत्य}
    उत
        {बाएँ=कोशिका[अ- 1]}

    नित्य केंद्र = कोशिका[अ]।
    नित्य दाएँ = असत्य।
    यदि(अ == (लंबाई - 1)) 
    {दाएँ  = असत्य} 
    उत
    {दाएँ=कोशिका[अ+ 1]}

    नित्य नईस्थिति = ((बाएँ और !केंद्र और !दाएँ) या (!बाएँ और केंद्र और दाएँ) या (!बाएँ और केंद्र और !दाएँ) या (!बाएँ और !केंद्र और दाएँ))। 
    नईकोशिका[अ] = नईस्थिति
  }

    नईकोशिका
}

कर्म कोशिकादिखाओ(कोशिका, लंबाई) {
  मान दिखाओ = ""। 
  चक्र (मान  अ = ० । से लंबाई) {
    यदि(कोशिका[अ])
        {दिखाओ = दिखाओ+"●"}
    उत
        {दिखाओ = दिखाओ+" "}
  }
  दिखाओ
}


मान कोशिका= [] । 
मान लंबाई = 100 ।
चक्र(मान अ = ० । से लंबाई)
  {
    कोशिका[अ] = असत्य
  }
  कोशिका[लंबाई/2] = सत्य

लेख(कोशिकादिखाओ(कोशिका, लंबाई))

चक्र (मान अ = 0।  से 50) {
  
  कोशिका = सूत्र११०(कोशिका, लंबाई)

  लेख(कोशिकादिखाओ(कोशिका, लंबाई))
}

